<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="10dp" >

    <TextView
        android:id="@+id/TVDFecha"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:maxLines="1"
        android:text="TextView"
        android:textAlignment="center"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/TVDDificultad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/TVDTag"
        android:layout_marginTop="5dp"
        android:text="TextView"
        android:textSize="16dp" />

    <TextView
        android:id="@+id/TVDTag"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/TVDFecha"
        android:layout_marginStart="33dp"
        android:text="TextView"
        android:textSize="16dp" />

    <TextView
        android:id="@+id/TVDRespCorrectas"
        android:layout_width="51dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="29dp"
        android:background="@drawable/textviewstyleazul"
        android:gravity="center"
        android:text="15"
        android:textColor="#FFFFFF"
        android:textSize="25dp" />

</RelativeLayout>
